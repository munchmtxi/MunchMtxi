{
  "name": "munch-mtxi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "audit:log": "powershell Get-Content -Path logs/audit.log -Wait",
    "backup": "node scripts/maintenance.js dailyBackup",
    "dev": "nodemon server.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "cross-env SKIP_CONFIG_VALIDATION=true yarn sequelize db:migrate",
    "migrate:undo": "cross-env SKIP_CONFIG_VALIDATION=true yarn sequelize db:migrate:undo",
    "monitor": "NODE_ENV=production node src/config/monitoring.js",
    "prepare": "husky install",
    "prestart": "yarn validate:env && yarn test:connections",
    "rotate:secrets": "node scripts/rotateSecrets.js",
    "seed": "cross-env SKIP_CONFIG_VALIDATION=true yarn sequelize db:seed:all",
    "seed:undo": "cross-env SKIP_CONFIG_VALIDATION=true yarn sequelize db:seed:undo:all",
    "sequelize": "sequelize-cli",
    "start": "node server.js",
    "test": "jest",
    "test:connections": "node scripts/testConnections.js",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "validate:env": "node scripts/validateEnv.js"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.0",
    "@sentry/node": "^8.51.0",
    "@sentry/profiling-node": "^8.51.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-status-monitor": "^1.3.4",
    "google-libphonenumber": "^3.2.40",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.13.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "web-push": "^3.4.5",
    "whatsapp-web.js": "^1.19.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@types/google-libphonenumber": "^7.4.30",
    "babel-jest": "^29.7.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}